{
    "name": "codexshaper/php-oauth2",
    "description": "OAuth2 authentication for PHP.",
    "keywords": [
        "php", 
        "authentication", 
        "oauth",
        "oauth2",
        "oauth2-authentication",
        "oauth2-server",
        "league-oauth2-server"
    ],
    "license": "MIT",
    "homepage": "https://github.com/Codexshaper/php-oauth2",
    "support": {
        "issues": "https://github.com/Codexshaper/php-oauth2/issues",
        "source": "https://github.com/Codexshaper/php-oauth2"
    },
    "authors": [
        {
            "name": "Md Abu Ahsan Basir",
            "email": "maab.career@gmail.com"
        }
    ],
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "codexshaper/php-database": "^1.0",
        "league/oauth2-server": "^8.1",
        "phpseclib/phpseclib": "^2.0",
        "illuminate/http": "^7.14",
        "symfony/psr-http-message-bridge": "^2.0",
        "nyholm/psr7": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
         "psr-4": {
           "CodexShaper\\OAuth2\\Server\\": "src/"
         }
    },
    "autoload-dev": {
        "psr-4": {
            "CodexShaper\\OAuth2\\Server\\": "src/",
            "CodexShaper\\OAuth2\\Server\\Test\\": "src/"
        }
    }
}